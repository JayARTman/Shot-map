<!-- log in form -->
<body>
    <script src="/js/log-in.js"></script>
        <main id="mainLogInContainer" class="d-flex justify-content-center p-4 m-4">
            <form>
                <h4>Enter User Information</h4>
                <div class="form-group row">
                    <label for="inputemail" class="col-sm-3 col-form-label">Email</label>
                    <div class="col-sm-10">
                    <input type="email" autocomplete="username"  class="form-control pb-1" id="inputEmail" placeholder="Email">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="inputPassword" class="col-sm-3 col-form-label">Password</label>
                    <div class="col-sm-10">
                    <input type="password" autocomplete="new-password" class="form-control pb-1" id="inputPassword" placeholder="Password">
                    </div>
                </div>
                <br/>
                <button id="logInBtn" type="button" class="btn btn-secondary w-100">Log In</button>
            </form>
        </main>  
</body>
